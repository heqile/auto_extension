{
    "name": "auto_login_extension",
    "version": "1.0",
    "description": "This extension is used to list logins for target site",
    "icons": {
        "16": "icon/icon16.png",
        "48": "icon/icon48.png",
       "128": "icon/icon128.png"
    },
    "background": {
        "scripts": ["js/background.js"],
        "persistent": false
    },
    "options_page": "html/options.html",
    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "http://*/*",
        "https://*/*"
    ],
    "web_accessible_resources": [
        "config/data.json",
        "html/list.html"
    ],
    "browser_action": {
        "default_tilte": "Auto Login",
        "default_popup": "html/popup.html"
    },
    "manifest_version": 2
}